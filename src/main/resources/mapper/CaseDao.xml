<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="learn.dao.mysql.CaseDao" >
    <!--填写映射-->
    <select id="findNameByDateCreated" parameterType="learn.dao.mysql.entity.Case" resultType="String">
